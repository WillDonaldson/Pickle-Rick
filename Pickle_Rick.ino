void draw()
{
  if (frame == 0)
   u8g.drawBitmapP( 0, 0, 16, 64, one);
  else if (frame == 1)
   u8g.drawBitmapP( 0, 0, 16, 64, two);
  else if(frame == 2)
   u8g.drawBitmapP( 0, 0, 16, 64, three);
  else if(frame == 3)
   u8g.drawBitmapP( 0, 0, 16, 64, four);
  else if(frame == 4)
   u8g.drawBitmapP( 0, 0, 16, 64, five);
  else if(frame == 5)
   u8g.drawBitmapP( 0, 0, 16, 64, six);
