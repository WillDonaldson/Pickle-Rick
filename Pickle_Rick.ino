void setup(void) {
 if (!SD.begin(SD_CARD)) {  //check SD Card is working
  return;  
 }
 audio.speakerPin = 9;   //pin speaker is connected to
 audio.volume(7);
}

void loop(void) {
  
 u8g.firstPage();
   do{
     draw();
   } while(u8g.nextPage());
 delay(4000);         //display rest face for 4secs before talking
 
 //speaking facial expressions
 frame=1;
 while(frame<6){
   if(frame==2 && sentence==0){             //start moving mouth (frame 1) before starting audio (frame 2), due to lag in OLED
    audio.play("PickRick.wav");             //if & else if statements alternate between 2 different audio samples
   }
   else if(frame==2 && sentence==1){ 
    audio.play("WLDdub.wav");
   }
   u8g.firstPage();
    do{
     draw();
   } while(u8g.nextPage());
   frame ++;
   delay(50);  
 }
 sentence=abs(sentence-1); //changes 1 to 0 and visa vera, alternates between the 2 audio samples
 frame=0;                  //rest facial expressions for next loop
}

